{
  "name": "api-gateway",
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=development && ts-node-dev --respawn --transpile-only  src/main.ts",
    "start:prod": "set NODE_ENV=production && node dist/apps/api-gateway/src/main",
    "build": "nest build",
    "postbuild": "cpx ../../packages/proto/*.proto ./dist/packages/proto/"
  },
  "dependencies": {
    "@foodapp/utils": "workspace:*",
    "@grpc/grpc-js": "^1.9.3",
    "@grpc/proto-loader": "^0.7.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "nestjs-pino": "^4.4.1",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0"
  }
}